<!DOCTYPE html>
<html>
<head>
  <title>Password Strength</title>
  <style>
    body {
      font-family: Arial;
      text-align: center;
      margin-top: 100px;
    }
    input {
      padding: 10px;
      width: 250px;
    }
    p {
      font-size: 18px;
    }
  </style>
</head>
<body>

<h2>Password Strength Checker</h2>
<input type="password" id="pass" placeholder="Enter password" onkeyup="check()">
<p id="msg"></p>

<script>
function check() {
  let p = document.getElementById("pass").value;
  let msg = document.getElementById("msg");

  if (p.length < 6) {
    msg.innerText = "Weak Password";
    msg.style.color = "red";
  } else if (p.length < 10) {
    msg.innerText = "Medium Password";
    msg.style.color = "orange";
  } else {
    msg.innerText = "Strong Password";
    msg.style.color = "green";
  }
}
</script>

</body>
</html>
